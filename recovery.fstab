# recovery.fstab for HTC Desire SV - codename magnids
# by joseph.heller@catch22.eu
#
# mount point	 fstype	device			[device2]

/boot		 emmc	/dev/block/mmcblk0p22
/cache		 ext4	/dev/block/mmcblk0p27
/data		 ext4	/dev/block/mmcblk0p26
/misc		 emmc	/dev/block/mmcblk0p17
/recovery	 emmc	/dev/block/mmcblk0p21
/sdcard     	 vfat   /dev/block/mmcblk1p1
/system     	 ext4   /dev/block/mmcblk0p25
/devlog          ext4   /dev/block/mmcblk0p28
/internal_sdcard vfat   /dev/block/mmcblk0p31

